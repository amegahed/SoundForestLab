{
	"desktop": {
		"show_app_launcher": true,
		"preferences": {
			"desktop_apps": {
				"phone": ["app_launcher"],
				"tablet": ["app_launcher"],
				"desktop": ["file_browser", "communicator"]
			},
			"pinned_apps": []
		}
	},

	"search": {
		"directory": "/Shared",
		"placeholder": "Search",
		"hidden": true
	},

	"settings": {
		"enable_notifications": true,
		"mute_sounds": false,
		"volume": 5
	},

	"dialogs": {
		"sizes": {
			"tiny": [600, 400],
			"small": [720, 480],
			"medium": [900, 600],
			"large": [1200, 800]
		},
		"scale": [0.75, 0.75],
		"margin": [100, 100]
	},

	"colors": [
		"red", "orange", "gold", "yellow", "green", "teal", "aqua", "cyan", "blue", "violet",
		"magenta", "brown", "grey"
	],

	"text": {

		"titles": [
			".title", ".jqplot-title",
			".app-bar",
			".context-menu",
			".heading",
			".popover-title",
			".dropzone",
			".app th", ".modal th", 
			".app .help.message"
		],

		"labels": [
			"label",
			".header label",
			".dropdown .form-label",
			".app .form-label", ".modal .form-label",
			".app .control-label", ".modal .control-label",
			".menu-bar .dropdown-toggle",
			".menu-bar .dropdown-menu li a",
			".nav-tabs",
			".btn",
			"input",
			"select"
		],

		"headings": [
			"h1", "h2", "h3"
		]
	},

	"help": {
		"show_app": true,
		"show_pdf": false,
		"show_topic": true,
		"show_contact": true
	},

	"contact": {
		"description": "For questions and comments, please fill out the contact form below: ",
		"subjects": [
			"Question",
			"Comment",
			"Feature Request",
			"Upgrading My Account",
			"Setting Up My Own Instance",
			"Bug Report",
			"Security Incident",
			"Other"
		],
		"label": "Submit Question"
	}
}